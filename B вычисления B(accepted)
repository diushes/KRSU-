point1=[a for a in input().split(' ')]

point1[0]=float(point1[0])
point1[1]=float(point1[1])

point2=[b for b in input().split(' ')]

point2[0]=float(point2[0])
point2[1]=float(point2[1])


katet1=abs(point1[0]-point2[0])
katet2=abs(point1[1]-point2[1])
gip=(katet1**2+katet2**2)**(1/2)
print(round(gip,3))
